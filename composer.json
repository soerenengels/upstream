{
  "name": "soerenengels/upstream",
  "description": "Upstream. Journalismus für faire Gesundheit",
  "type": "project",
  "keywords": [
    "kirby",
    "cms",
    "starterkit"
  ],
  "homepage": "https://upstream-newsletter.de/",
  "authors": [
    {
      "name": "Sören Engels",
      "email": "mail@soerenengels.de",
      "homepage": "https://soerenengels.de/"
    }
  ],
  "require": {
    "php": ">=7.4.0 <8.4.0",
    "johannschopplich/kirby-hashed-assets": "^3.0",
    "afbora/kirby-minify-html": "^2.0",
    "tobimori/kirby-seo": "^1.0.0-rc.7",
    "bnomei/kirby3-dotenv": "^2.2",
    "soerenengels/kirby-steady": "^0.3.0",
    "getkirby/cms": "^4.3",
    "tobimori/kirby-icon-field": "^2.0",
    "junohamburg/kirby-visual-block-selector": "^2.1",
    "tobimori/kirby-dreamform": "^1.3",
    "distantnative/retour-for-kirby": "^5.2"
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}
